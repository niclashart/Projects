      \strng{namehash}{f1678cfa142d8029151d4faacf40f176}
      \strng{fullhash}{0ac64c8a2a8c144751e6f1d6573b5725}
      \strng{bibnamehash}{f1678cfa142d8029151d4faacf40f176}
      \strng{authorbibnamehash}{f1678cfa142d8029151d4faacf40f176}
